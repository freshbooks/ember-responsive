{
  "name": "ember-responsive",
  "version": "4.0.2",
  "description": "An ember-cli addon that gives you a simple, Ember-aware way of dealing with media queries.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/freshbooks/ember-responsive",
  "engines": {
    "node": "10.* || >= 12"
  },
  "author": "FreshBooks",
  "license": "MIT",
  "scripts": {
