  "engines": {
    "node": "^14.18.0 || ^16.0.0 || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "keywords": [
    "ember-addon",
    "media query"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://freshbooks.github.io/ember-responsive/"
  }
